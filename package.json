    "@valiantlynx/tsconfig": "^0.0.21",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
